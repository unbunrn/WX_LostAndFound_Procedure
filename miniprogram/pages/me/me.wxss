 .unlogin_body{
   background-color: #fff;
 }
 
 .unlogin{
   display: flex;
   justify-content: center;
   align-items: center;
   flex-direction: column;
   padding-top: 40%;
 }

.unlogin .unlogin_image{
  width: 150rpx;
  height: 150rpx;
}

.unlogin .unlogin_button{
  margin-top: 50rpx; 
  width: 80%;
  background-color: #ff70b4;
  color: #fff;
  height: 90rpx;

}

.info{
  background-color: #ff70b4;
  display: flex;
  align-items: center;
  justify-items: center;
  flex-direction: column;
  padding: 100rpx 0;
}

.info .avatarUrl{
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;
}

.info .nickName{
  color: #fff;
  margin-top: 30rpx;
}
.cell_body{
  position: relative;

}
.cell_list{
  display: flex;
  flex-direction: column;
  align-items: center;
  position: absolute;
  width: 100%;
  top: -60rpx;
}

.cell{
  display: flex;
  justify-content: space-between;
  width: 85%;
  background-color: rgb(250, 244, 244);
  align-items: center;
  padding: 30rpx 20rpx;
  border-radius: 20rpx;
  margin-top: 20rpx;
   
}
.cell_left{
  display: flex;
  align-items: center;
}

.cell_left .cell_left_image{
  width: 60rpx;
  height: 60rpx;
}

.cell_left .cell_left_text{
  margin-left: 20rpx;
}
 
.cell_right .cell_right_image{
  width: 40rpx;
  height: 40rpx;
}