/* pages/register/register.wxss */.body{
  display: flex;
  flex-direction: column;
  align-items: center;
}

.logo{
  width: 350rpx;
  height: 350rpx;
  border-radius: 50%;
  padding-top: 100rpx;
}
.container{
  margin-top: 50rpx;
  width: 70%;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.container .input{
  width: 100%;
  border-bottom: 2rpx solid #ddbbbb;
  margin-top: 60rpx;
}

.container .to_login{
  margin-right: auto;
  color: #ff70b4;
  margin-top: 30rpx;
}

.container .register_btn{
  width: 90%! important;
  margin-top: 80rpx;
  border-radius: 40rpx;
  background-color: rgb(158, 161, 68);
  color: rgb(255, 255, 255);
}